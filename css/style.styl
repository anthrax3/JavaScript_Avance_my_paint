bgcolor = white
bordercolor = black

*
	margin 0
	padding 0

body
	background url('../img/background.jpg') no-repeat center center fixed
	background-size cover

#canvas
	border 2px solid #999
	border-radius 5px
	margin auto auto
	display block
	background bgcolor
	cursor crosshair

nav
	height 100%
	width 800px
	background-color #EEE
	text-align left
	overflow hidden
	margin 0 auto
	padding 5px 0

input
	vertical-align top

#size
	width 100px

span, #text, #undo, #redo
	vertical-align top
	line-height 32px

button, input
	display inline-block
	width 32px
	height 32px
	background-color inherit
	border-radius 5px
	border none
	&:hover
		background-color #FFF
	&:active
		background-color #CCC

.active
	background-color red!important

#line
	background-image url('data:image/png;base64,')
